  "license": "MIT",
  "homepage": "https://github.com/romelperez/arwes",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run test-lint && TRAVIS=true npm run test-dev",
    "test-lint": "eslint --cache --quiet ./src",
    "test-dev": "karma start",
    "compile": "npm run compile-clean && npm run compile-run",
    "compile-clean": "rimraf ./lib/*",
    "compile-run": "babel --ignore '*.test.js' src --out-dir lib",
    "play": "webpack-dev-server",
    "script-api": "node ./scripts/api",
    "site-dev": "npm run script-api && next -p 7000",
    "site-build": "npm run script-api && next build",
    "site-start": "next start -p 17000",
    "site-deploy": "pm2 startOrRestart deploy.json --env production"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },

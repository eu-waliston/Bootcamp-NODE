    "babel-preset-stage-1": "^6.22.0",
    "chai": "^4.1.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^3.18.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^1.1.6",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^2.2.0",
